@model IEnumerable<FreeLance.Controllers.EmployerController.ArchivedContractViewModel>
@{
    ViewBag.Title = "Архив";
}

<h2>@ViewBag.Title</h2>
        <h3>Выполненные контракты</h3>
        <div class="list-group">
            @foreach ( var contract in @Model)
			{
            <a href="#" class="list-group-item">
                <h4 class="list-group-item-heading">Контракт по задаче <b>@contract.Name</b></h4>
                <p class="list-group-item-text"><b>Исполнитель: </b>@contract.FreelancerName</p>
                <p class="list-group-item-text">@contract.Details</p>
            </a>
			}
        </div>
