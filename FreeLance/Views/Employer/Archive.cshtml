
@{
	ViewBag.Title = "Архив";
}

<h2>@ViewBag.Title</h2>
<div class="row">
	<div class="col-md-10">
		<h3>Выполненные контракты</h3>
		<div class="list-group">
			@for (var i = 0; i < 2; i++)
			{
				<a href="#" class="list-group-item">
					<h4 class="list-group-item-heading">Контракт @i</h4>
					<p class="list-group-item-text">Краткое описание</p>
				</a>
			}
		</div>
	</div>
</div>