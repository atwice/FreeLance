@model List<FreeLance.Models.ContractModels>
@{
    ViewBag.Title = "Новые отзывы";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Freelancer documents</title>
</head>
<body>
<div class="row">
    <div class="col-md-6">
        @if (Model.Count == 0)
        {
            <p>Все отзывы прочитаны.</p>
        }
        else
        {
            foreach (var contract in Model)
            {
                Html.RenderPartial("~/Views/Comment/SmallViewCoordinator.cshtml", contract);
            }
        }
    </div>
</div>
</body>