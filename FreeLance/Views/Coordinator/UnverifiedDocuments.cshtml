@model List<FreeLance.Models.DocumentPackageModels>
@{
    ViewBag.Title = "Документы для подтверждения";
}


<div class="row">
	<h2>Документы для подтверждения:</h2>
    <div class="col-md-6" style="margin-top: 15px">
        @if (Model.Count == 0)
        {
            <p>Все документы фрилансеров подтверждены. Отличная работа!</p>
        }
        else
        {
            foreach (var doc in Model)
            {
                Html.RenderPartial("~/Views/Freelancer/DocumentSmallView.cshtml", doc);
            }
        }
    </div>
</div>